#include<iostream>
using namespace std;
void swap(int &x, int &y){
	int temp;
	temp=x;
	x=y;
	y=temp;
	
	
}
int main(){
	int a,b;
	cout<<"The value of a is "; //pass by reference
	cin>>a;
	cout<<"The value of b is ";
	cin>>b;
	
	swap(a,b);
	cout<<"After swapping, the value of a becomes "<<a<<endl;
	cout<<"After swapping, the value of b becomes "<<b<<endl;
	
	
}
